{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "es2015",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@until-failure-app/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "./app.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
